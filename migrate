dest=c11node10
telnetport=4446
expno=4

ssh $dest /root/qemu/listen-daemon

#sleep 3

./runvm-daemon

sleep 20

echo starting dstat ...
ssh localhost -p 5555 "dstat -cmdngy > dstat/$expno.log &" 

# run sysbench in background
echo starting sysbench ...
ssh localhost -p 5555 "/root/run > sysbench/$expno.log &"

sleep 10

echo starting migration ...
#./ncmigrate 
./ncmigrate > sb$expno.log

grep time -a sb$expno.log

sleep 10

./stopvm $dest $telnetport

